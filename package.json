{
  "icon": "./icons/icon.png",
  "name": "rgss-script-editor",
  "displayName": "RGSS Script Editor",
  "description": "An extension that allows to use Visual Studio Code as the code editor for RPG Maker game projects",
  "author": {
    "name": "SnowSzn",
    "url": "https://github.com/SnowSzn"
  },
  "publisher": "SnowSzn",
  "license": "GPL-3.0-only",
  "keywords": [
    "Ruby Game Scripting System",
    "RGSS",
    "RGSS1",
    "RGSS2",
    "RGSS3",
    "RPGMaker",
    "RPG Maker Script Editor",
    "RPG Maker Script Extractor",
    "RPG Maker",
    "RPG Maker XP",
    "RPG Maker VX",
    "RPG Maker VX Ace"
  ],
  "repository": {
    "url": "https://github.com/SnowSzn/rgss-script-editor"
  },
  "bugs": {
    "url": "https://github.com/SnowSzn/rgss-script-editor/issues"
  },
  "version": "1.5.2",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:Game.ini"
  ],
  "l10n": "./l10n",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rgss-script-editor.setProjectFolder",
        "title": "%command.setProjectFolder.title%",
        "category": "RGSS Script Editor",
        "icon": "$(folder)"
      },
      {
        "command": "rgss-script-editor.openProjectFolder",
        "title": "%command.openProjectFolder.title%",
        "category": "RGSS Script Editor",
        "icon": "$(folder-opened)"
      },
      {
        "command": "rgss-script-editor.extractScripts",
        "title": "%command.extractScripts.title%",
        "category": "RGSS Script Editor",
        "icon": "$(arrow-down)"
      },
      {
        "command": "rgss-script-editor.importScripts",
        "title": "%command.importScripts.title%",
        "category": "RGSS Script Editor",
        "icon": "$(file-add)"
      },
      {
        "command": "rgss-script-editor.createScriptLoader",
        "title": "%command.createScriptLoader.title%",
        "category": "RGSS Script Editor",
        "icon": "$(note)"
      },
      {
        "command": "rgss-script-editor.createBackupBundleFile",
        "title": "%command.createBackupBundleFile.title%",
        "category": "RGSS Script Editor",
        "icon": "$(archive)"
      },
      {
        "command": "rgss-script-editor.createBackupLoadOrder",
        "title": "%command.createBackupLoadOrder.title%",
        "category": "RGSS Script Editor",
        "icon": "$(archive)"
      },
      {
        "command": "rgss-script-editor.createBundleFile",
        "title": "%command.createBundleFile.title%",
        "category": "RGSS Script Editor",
        "icon": "$(package)"
      },
      {
        "command": "rgss-script-editor.createSelectedBundleFile",
        "title": "%command.createSelectedBundleFile.title%",
        "category": "RGSS Script Editor",
        "icon": "$(package)"
      },
      {
        "command": "rgss-script-editor.compileBundleFile",
        "title": "%command.compileBundleFile.title%",
        "category": "RGSS Script Editor",
        "icon": "$(package)"
      },
      {
        "command": "rgss-script-editor.runGame",
        "title": "%command.runGame.title%",
        "category": "RGSS Script Editor",
        "icon": "$(run)"
      },
      {
        "command": "rgss-script-editor.processGameException",
        "title": "%command.processGameException.title%",
        "category": "RGSS Script Editor",
        "icon": "$(warning)"
      },
      {
        "command": "rgss-script-editor.chooseEditorMode",
        "title": "%command.chooseEditorMode.title%",
        "category": "RGSS Script Editor",
        "icon": "$(insert)"
      },
      {
        "command": "rgss-script-editor.openLoadOrder",
        "title": "%command.openLoadOrder.title%",
        "category": "RGSS Script Editor",
        "icon": "$(tasklist)"
      },
      {
        "command": "rgss-script-editor.sectionCreate",
        "title": "%command.sectionCreate.title%",
        "category": "RGSS Script Editor",
        "icon": "$(new-file)"
      },
      {
        "command": "rgss-script-editor.sectionDelete",
        "title": "%command.sectionDelete.title%",
        "category": "RGSS Script Editor",
        "icon": "$(trash)"
      },
      {
        "command": "rgss-script-editor.sectionRename",
        "title": "%command.sectionRename.title%",
        "category": "RGSS Script Editor",
        "icon": "$(edit)"
      },
      {
        "command": "rgss-script-editor.sectionMove",
        "title": "%command.sectionMove.title%",
        "category": "RGSS Script Editor",
        "icon": "$(move)"
      },
      {
        "command": "rgss-script-editor.sectionCut",
        "title": "%command.sectionCut.title%",
        "category": "RGSS Script Editor",
        "icon": "$(clippy)"
      },
      {
        "command": "rgss-script-editor.sectionCopy",
        "title": "%command.sectionCopy.title%",
        "category": "RGSS Script Editor",
        "icon": "$(clippy)"
      },
      {
        "command": "rgss-script-editor.sectionPaste",
        "title": "%command.sectionPaste.title%",
        "category": "RGSS Script Editor",
        "icon": "$(clippy)"
      },
      {
        "command": "rgss-script-editor.sectionToggleLoad",
        "title": "%command.sectionToggleLoad.title%",
        "category": "RGSS Script Editor",
        "icon": "$(arrow-swap)"
      },
      {
        "command": "rgss-script-editor.sectionToggleCollapse",
        "title": "%command.sectionToggleCollapse.title%",
        "category": "RGSS Script Editor",
        "icon": "$(arrow-both)"
      },
      {
        "command": "rgss-script-editor.sectionRevealInVSCodeExplorer",
        "title": "%command.sectionRevealInVSCodeExplorer.title%",
        "category": "RGSS Script Editor",
        "icon": "$(eye)"
      },
      {
        "command": "rgss-script-editor.sectionOpenInExplorer",
        "title": "%command.sectionOpenInExplorer.title%",
        "category": "RGSS Script Editor",
        "icon": "$(search)"
      },
      {
        "command": "rgss-script-editor.sectionCopyAbsolutePath",
        "title": "%command.sectionCopyAbsolutePath.title%",
        "category": "RGSS Script Editor",
        "icon": "$(copy)"
      },
      {
        "command": "rgss-script-editor.sectionCopyRelativePath",
        "title": "%command.sectionCopyRelativePath.title%",
        "category": "RGSS Script Editor",
        "icon": "$(copy)"
      }
    ],
    "configuration": [
      {
        "title": "RGSS Script Editor",
        "properties": {
          "rgssScriptEditor.debug.logToConsole": {
            "type": "boolean",
            "default": true,
            "description": "%configuration.debug.logToConsole.description%",
            "order": 0
          },
          "rgssScriptEditor.debug.logToFile": {
            "type": "boolean",
            "default": false,
            "description": "%configuration.debug.logToFile.description%",
            "order": 1
          },
          "rgssScriptEditor.extension.quickStart": {
            "type": "boolean",
            "default": true,
            "description": "%configuration.extension.quickStart.description%",
            "order": 10
          },
          "rgssScriptEditor.extension.autoReveal": {
            "type": "boolean",
            "default": true,
            "description": "%configuration.extension.autoReveal.description%",
            "order": 11
          },
          "rgssScriptEditor.extension.filesEndOfLine": {
            "type": "string",
            "enum": [
              "auto",
              "\r\n",
              "\n"
            ],
            "enumDescriptions": [
              "%configuration.extension.filesEndOfLine.enumDescriptionAuto%",
              "%configuration.extension.filesEndOfLine.enumDescriptionCRLF%",
              "%configuration.extension.filesEndOfLine.enumDescriptionLF%"
            ],
            "default": "auto",
            "description": "%configuration.extension.filesEndOfLine.description%",
            "order": 12
          },
          "rgssScriptEditor.extension.insertEncodingComment": {
            "type": "boolean",
            "default": false,
            "description": "%configuration.extension.insertEncodingComment.description%",
            "order": 13
          },
          "rgssScriptEditor.extension.recreateScriptLoader": {
            "type": "boolean",
            "default": true,
            "description": "%configuration.extension.recreateScriptLoader.description%",
            "order": 14
          },
          "rgssScriptEditor.extension.scriptNameValidation": {
            "type": "string",
            "enum": [
              "auto",
              "always",
              "never"
            ],
            "enumDescriptions": [
              "%configuration.extension.scriptNameValidation.enumDescriptionAuto%",
              "%configuration.extension.scriptNameValidation.enumDescriptionAlways%",
              "%configuration.extension.scriptNameValidation.enumDescriptionNever%"
            ],
            "default": "auto",
            "description": "%configuration.extension.scriptNameValidation.description%",
            "order": 18
          },
          "rgssScriptEditor.external.backUpsFolder": {
            "type": "string",
            "default": "./.rgss-script-editor/backups",
            "description": "%configuration.external.backUpsFolder.description%",
            "order": 30
          },
          "rgssScriptEditor.external.loadOrderBackUpsFolder": {
            "type": "string",
            "default": "./.rgss-script-editor/backups/load orders",
            "description": "%configuration.external.loadOrderBackUpsFolder.description%",
            "order": 32
          },
          "rgssScriptEditor.external.scriptsFolder": {
            "type": "string",
            "default": "./Scripts",
            "description": "%configuration.external.scriptsFolder.description%",
            "order": 35
          },
          "rgssScriptEditor.external.extensionLogFileFolder": {
            "type": "string",
            "default": "./.rgss-script-editor",
            "description": "%configuration.external.extensionLogFileFolder.description%",
            "order": 40
          },
          "rgssScriptEditor.external.gameLogFileFolder": {
            "type": "string",
            "default": "./.rgss-script-editor",
            "description": "%configuration.external.gameLogFileFolder.description%",
            "order": 41
          },
          "rgssScriptEditor.external.scriptsCompileFolder": {
            "type": "string",
            "default": "./.rgss-script-editor/dist",
            "description": "%configuration.external.scriptsCompileFolder.description%",
            "order": 45
          },
          "rgssScriptEditor.gameplay.gameExecutablePath": {
            "type": "string",
            "default": "./Game.exe",
            "description": "%configuration.gameplay.gameExecutablePath.description%",
            "order": 50
          },
          "rgssScriptEditor.gameplay.useWine": {
            "type": "string",
            "default": "wine",
            "description": "%configuration.gameplay.useWine.description%",
            "order": 51
          },
          "rgssScriptEditor.gameplay.runGameBehavior": {
            "type": "string",
            "enum": [
              "nothing",
              "kill and run",
              "allow multiple"
            ],
            "enumDescriptions": [
              "%configuration.gameplay.runGameBehavior.enumDescriptionNothing%",
              "%configuration.gameplay.runGameBehavior.enumDescriptionKillAndRun%",
              "%configuration.gameplay.runGameBehavior.enumDescriptionAllowMultiple%"
            ],
            "default": "nothing",
            "description": "%configuration.gameplay.runGameBehavior.description%",
            "order": 55
          },
          "rgssScriptEditor.gameplay.automaticArgumentsDetection": {
            "type": "boolean",
            "default": true,
            "description": "%configuration.gameplay.automaticArgumentsDetection.description%",
            "order": 60
          },
          "rgssScriptEditor.gameplay.editorTestMode": {
            "type": "boolean",
            "default": true,
            "description": "%configuration.gameplay.editorTestMode.description%",
            "order": 61
          },
          "rgssScriptEditor.gameplay.nativeConsole": {
            "type": "boolean",
            "default": true,
            "description": "%configuration.gameplay.nativeConsole.description%",
            "order": 62
          },
          "rgssScriptEditor.gameplay.customArguments": {
            "type": "string",
            "default": "",
            "description": "%configuration.gameplay.customArguments.description%",
            "order": 63
          },
          "rgssScriptEditor.gameplay.gameExceptionAutoProcess": {
            "type": "boolean",
            "default": false,
            "description": "%configuration.gameplay.gameExceptionAutoProcess.description%",
            "order": 70
          },
          "rgssScriptEditor.gameplay.gameExceptionShowInEditor": {
            "type": "boolean",
            "default": true,
            "description": "%configuration.gameplay.gameExceptionShowInEditor.description%",
            "order": 71
          }
        }
      }
    ],
    "keybindings": [
      {
        "command": "rgss-script-editor.setProjectFolder",
        "key": "F6",
        "when": "workspaceFolderCount > 0"
      },
      {
        "command": "rgss-script-editor.runGame",
        "key": "F12",
        "when": "rgss-script-editor.openedFolder"
      },
      {
        "command": "rgss-script-editor.compileBundleFile",
        "key": "CTRL+F5",
        "when": "rgss-script-editor.extractedScripts"
      },
      {
        "command": "rgss-script-editor.sectionCreate",
        "key": "insert",
        "when": "focusedView == 'rgss-script-editor.editorView'"
      },
      {
        "command": "rgss-script-editor.sectionDelete",
        "key": "delete",
        "when": "focusedView == 'rgss-script-editor.editorView'"
      },
      {
        "command": "rgss-script-editor.sectionRename",
        "key": "F2",
        "when": "focusedView == 'rgss-script-editor.editorView'"
      },
      {
        "command": "rgss-script-editor.sectionCut",
        "key": "CTRL+X",
        "when": "focusedView == 'rgss-script-editor.editorView'"
      },
      {
        "command": "rgss-script-editor.sectionCopy",
        "key": "CTRL+C",
        "when": "focusedView == 'rgss-script-editor.editorView'"
      },
      {
        "command": "rgss-script-editor.sectionPaste",
        "key": "CTRL+V",
        "when": "focusedView == 'rgss-script-editor.editorView'"
      },
      {
        "command": "rgss-script-editor.sectionToggleLoad",
        "key": "F4",
        "when": "focusedView == 'rgss-script-editor.editorView'"
      },
      {
        "command": "rgss-script-editor.sectionCopyAbsolutePath",
        "key": "F7",
        "when": "focusedView == 'rgss-script-editor.editorView'"
      },
      {
        "command": "rgss-script-editor.sectionCopyRelativePath",
        "key": "F8",
        "when": "focusedView == 'rgss-script-editor.editorView'"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rgss-script-editor",
          "icon": "./icons/views/icon.svg",
          "title": "RGSS Script Editor"
        }
      ]
    },
    "views": {
      "rgss-script-editor": [
        {
          "id": "rgss-script-editor.editorView",
          "icon": "./icons/views/icon.svg",
          "contextualTitle": "RGSS Script Editor",
          "name": "Editor"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "rgss-script-editor.editorView",
        "contents": "%viewsWelcome.contents1%",
        "when": "workspaceFolderCount <= 0"
      },
      {
        "view": "rgss-script-editor.editorView",
        "contents": "%viewsWelcome.contents2%",
        "when": "workspaceFolderCount == 1 && !rgss-script-editor.openedFolder"
      },
      {
        "view": "rgss-script-editor.editorView",
        "contents": "%viewsWelcome.contents3%",
        "when": "workspaceFolderCount > 1 && !rgss-script-editor.openedFolder"
      },
      {
        "view": "rgss-script-editor.editorView",
        "contents": "%viewsWelcome.contents4%",
        "when": "rgss-script-editor.openedFolder && !rgss-script-editor.extractedScripts"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "rgss-script-editor.chooseEditorMode",
          "when": "view == rgss-script-editor.editorView",
          "group": "navigation"
        },
        {
          "command": "rgss-script-editor.openLoadOrder",
          "when": "view == rgss-script-editor.editorView",
          "group": "navigation"
        },
        {
          "command": "rgss-script-editor.sectionCreate",
          "when": "view == rgss-script-editor.editorView",
          "group": "1"
        },
        {
          "command": "rgss-script-editor.processGameException",
          "when": "view == rgss-script-editor.editorView",
          "group": "2"
        },
        {
          "command": "rgss-script-editor.createScriptLoader",
          "when": "view == rgss-script-editor.editorView",
          "group": "3"
        },
        {
          "command": "rgss-script-editor.createBundleFile",
          "when": "view == rgss-script-editor.editorView",
          "group": "4"
        },
        {
          "command": "rgss-script-editor.createBackupBundleFile",
          "when": "view == rgss-script-editor.editorView",
          "group": "4"
        },
        {
          "command": "rgss-script-editor.createBackupLoadOrder",
          "when": "view == rgss-script-editor.editorView",
          "group": "4"
        },
        {
          "command": "rgss-script-editor.createSelectedBundleFile",
          "when": "view == rgss-script-editor.editorView",
          "group": "4"
        },
        {
          "command": "rgss-script-editor.importScripts",
          "when": "view == rgss-script-editor.editorView",
          "group": "5"
        }
      ],
      "view/item/context": [
        {
          "command": "rgss-script-editor.sectionRename",
          "when": "view == rgss-script-editor.editorView",
          "group": "1"
        },
        {
          "command": "rgss-script-editor.sectionToggleLoad",
          "when": "view == rgss-script-editor.editorView",
          "group": "2"
        },
        {
          "command": "rgss-script-editor.sectionCreate",
          "when": "view == rgss-script-editor.editorView",
          "group": "3"
        },
        {
          "command": "rgss-script-editor.sectionDelete",
          "when": "view == rgss-script-editor.editorView",
          "group": "3"
        },
        {
          "command": "rgss-script-editor.sectionCut",
          "when": "view == rgss-script-editor.editorView",
          "group": "4"
        },
        {
          "command": "rgss-script-editor.sectionCopy",
          "when": "view == rgss-script-editor.editorView",
          "group": "4"
        },
        {
          "command": "rgss-script-editor.sectionPaste",
          "when": "view == rgss-script-editor.editorView",
          "group": "4"
        },
        {
          "command": "rgss-script-editor.sectionCopyAbsolutePath",
          "when": "view == rgss-script-editor.editorView",
          "group": "6"
        },
        {
          "command": "rgss-script-editor.sectionCopyRelativePath",
          "when": "view == rgss-script-editor.editorView",
          "group": "6"
        },
        {
          "command": "rgss-script-editor.sectionRevealInVSCodeExplorer",
          "when": "view == rgss-script-editor.editorView",
          "group": "7"
        },
        {
          "command": "rgss-script-editor.sectionOpenInExplorer",
          "when": "view == rgss-script-editor.editorView",
          "group": "7"
        },
        {
          "command": "rgss-script-editor.createSelectedBundleFile",
          "when": "view == rgss-script-editor.editorView",
          "group": "8"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "generate-package": "vsce package --out ./dist",
    "l10n-gen": "npx @vscode/l10n-dev export --outDir ./l10n ./src"
  },
  "dependencies": {
    "@hyrious/marshal": "^0.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "^22.15.18",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^9.26.0",
    "glob": "^11.0.2",
    "mocha": "^11.2.2",
    "typescript": "^5.2.2"
  }
}